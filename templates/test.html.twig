{% extends 'base.html.twig'%}
{% block body %}
    <header style="background: linear-gradient(-89deg, #AED741, #8fd16f, #53abab, #65CFCB);padding-bottom: 13px;">
        <div class="container-fluid" style="background: #ffffff;padding-top: 10px;padding-bottom: 10px;">
            <div class="row">
                <div class="col-md-1 col-lg-3 col-xl-3 col-xxl-3 d-flex d-sm-flex d-md-flex d-xxl-flex justify-content-center align-items-center justify-content-sm-center align-items-sm-center justify-content-md-center align-items-md-center justify-content-xxl-center align-items-xxl-center"><img src="{{ asset('build/images/logo_F-E.png') }}" style="width: 50px;margin-top: 10px;margin-bottom: 10px;"></div>
                <div class="col-md-7 col-lg-6 col-xxl-6 d-flex d-sm-flex d-xxl-flex justify-content-center align-items-center justify-content-sm-center align-items-sm-center justify-content-xxl-center"><img src="{{ asset('build/images/text_F-E.png') }}" style="max-width: 400px;width: 100%;"></div>
                <div class="col-sm-12 col-md-4 col-lg-3 col-xl-3 col-xxl-3 d-flex justify-content-around align-items-center align-items-xxl-center"><img src="{{ asset('build/images/Logo_Funded_EU.png') }}" style="max-height: 52px;max-width: 242px;min-width: 150px;margin-top: 10px;"><img src="{{ asset('build/images/Logo_Horizon_EU.png') }}" style="max-width: 70px;margin-top: 10px;"></div>
            </div>
        </div>
    </header>

    <div class="container-fluid d-flex d-sm-flex d-md-flex d-xxl-flex justify-content-center align-items-center justify-content-sm-center align-items-sm-center justify-content-md-center align-items-md-center justify-content-xxl-center align-items-xxl-center" style="height: 92%;max-height: 100%;background: url({{ asset('build/images/F-E_BG.png') }}) no-repeat;background-size: cover;">
        {% block div %}{% endblock %}
    </div>
{% endblock %}